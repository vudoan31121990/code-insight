{
	"hosting": {
		"public": "build",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"rewrites": [
			{
				"source": "**",
				"destination": "/index.html"
			},
			{
				"source": "/api/**",
				"destination": "https://code-insight-api-dot-code-insight-dev.uc.r.appspot.com/api/"
			}
		],
		"env": {
			"REACT_APP_BASE_URL": "https://code-insight-api-dot-code-insight-dev.uc.r.appspot.com/api"
		}
	}
}
